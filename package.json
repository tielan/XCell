{
  "name": "xcell_app",
  "version": "1.0.24",
  "private": true,
  "author": "tielan.fu",
  "description": "the xcell app",
  "scripts": {
    "start": "npm run electron:serve",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "npm run electron:build && npm run open",
    "l": "npm run electron:build-l",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build -w",
    "electron:build-l": "vue-cli-service electron:build -l",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "open": "start dist_electron",
    "icons": "electron-icon-builder --input=./public/icons/icon.png --output=public --flatten"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "electron": "^11.4.12",
    "electron-log": "^4.3.0",
    "electron-store": "^8.0.0",
    "element-ui": "^2.15.5",
    "lodash": "^4.17.21",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.6.11"
  }
}
